# Frontend Environment Variables for MadMax
# Copy this content to your frontend .env file

# Authentication API URL (Auth Service)
VITE_BACKEND_URL=https://madmax-production.up.railway.app

# Music API URL (Music Service)
VITE_MUSIC_API=https://your-music-service-url.up.railway.app

# Notification API URL (Notification Service)
VITE_NOTIFY_API=https://your-notification-service-url.up.railway.app

# For local development:
# VITE_BACKEND_URL=http://localhost:3001
# VITE_MUSIC_API=http://localhost:3000
# VITE_NOTIFY_API=http://localhost:3002
